<TaskOptions>
  <TaskOptions isEnabled="false">
    <option name="name" value="Slim"/>
    <option name="description" value="Renders Slim templates using the slimrb gem executable"/>
    <option name="program" value="~/.gem/ruby/2.1.2/bin/slimrb"/>
    <option name="arguments" value="-p $FilePath$ --trace"/>
    <option name="fileExtension" value="slim"/>
    <option name="workingDir" value="$ProjectFileDir$"/>
    <option name="output" value="$ProjectFileDir$/$FileNameWithoutExtension$.html"/>
    <option name="immediateSync" value=""/>
    <option name="trackOnlyRoot"/>
    <option name="exitCodeBehavior" value="ERROR"/>
    <option name="checkSyntaxErrors"/>
    <option name="outputFromStdout" value=""/>
    <option name="passParentEnvs" value=""/>
    <option name="scopeName" value="Project Files"/>
    <option name="outputFilters">
      <array/>
    </option>
    <envs/>
  </TaskOptions>
</TaskOptions>